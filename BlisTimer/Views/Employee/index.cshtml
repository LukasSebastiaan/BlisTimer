@model List<BlisTimer.Models.Employee>

@{

}

<style>
    .main{
        background-color: white;
    }
</style>
<div class="main">
    <h1>Employee </h1>
    <table class="table">
        <thead> 
            <tr>
                <th>Name</th>
                <th>Lastname</th>
                <th>Id</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var employee in Model)
            {
                <tr>
                    <td>@employee.Name
                        | Projects: 


                    </td>
                    <td>@employee.LastName</td>
                    <td>@employee.Id</td>
                </tr>
            }
        </tbody>
    </table>
</div>
